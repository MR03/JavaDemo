<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="pw.mr03.oktalia.mapper.AdminMapper">
    <resultMap id="AdminResultMap" type="pw.mr03.oktalia.entity.Admin">
        <id column="id" property="id" jdbcType="INTEGER" />
        <result column="realname" property="realname" jdbcType="VARCHAR" />
        <result column="password" property="password" jdbcType="VARCHAR" />
    </resultMap>
    <select id="getById" resultMap="AdminResultMap" parameterType="java.lang.Integer">
        select
        realname
        from ok_admin
        where id = #{id}
    </select>
    <insert id="getByTest" resultMap="AdminResultMap" parameterType="java.lang.Integer">
        select
        password
        from ok_admin
        where id = #{id}
    </insert>
</mapper>