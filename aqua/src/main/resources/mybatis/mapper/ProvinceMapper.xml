<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="pw.mr03.mapper.ProvinceMapper">
    <insert id="insertProvinceCell" parameterType="pw.mr03.entity.Province" useGeneratedKeys="true"
            keyProperty="id">
        insert into ok_province (province_name, code)
        values (#{name},#{code})
    </insert>
</mapper>