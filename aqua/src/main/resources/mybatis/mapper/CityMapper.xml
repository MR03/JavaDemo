<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="pw.mr03.mapper.CityMapper">
<insert id="insertCityCell" parameterType="pw.mr03.entity.City" useGeneratedKeys="true"
        keyProperty="id">
    insert into ok_city (city_name, province_code, code)
    values (#{name}, #{provinceCode}, #{code})
</insert>
</mapper>